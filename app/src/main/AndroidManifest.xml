<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.davidkim.sleepapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fourseveneight" />
        <activity
            android:name=".SoundsActivity"
            android:label="@string/title_activity_sounds"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GraphicsActivity"
            android:label="@string/title_activity_graphics"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VideosActivity"
            android:label="@string/title_activity_videos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".colors" />
        <activity android:name=".bread" />
        <activity android:name=".cheese"></activity>
    </application>

</manifest>